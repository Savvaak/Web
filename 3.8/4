function userCities(users) {
    let good = {};
    for(let i = 0; i < users.length;i++){
        let a = users[i].split(': ');
        let b = a[1].split(', ');
        for(let j = 0; j < b.length; j++){
            b[j] = b[j].charAt(0).toUpperCase() + b[j].slice(1).toLowerCase();
        }
        let arr = b.filter((item, index) => b.indexOf(item) === index);
        good[a[0]] = arr.sort();
    }
    return good;
}




